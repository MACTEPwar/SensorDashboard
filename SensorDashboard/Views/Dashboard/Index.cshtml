    // Инициализация DatePicker`а
    function initDatePicker() {
        $("[id^='datetimepicker'").datetimepicker({
            locale: 'ru'
        });
    }

    // Рисует путь на карте
    function drawPath(path) {
        if (flightPath !== undefined) flightPath.setMap(null);
        flightPath = new google.maps.Polyline({
            path: path,
            geodesic: true,
            strokeColor: '#FF0000',
            strokeOpacity: 0.5,
            strokeWeight: 8
        });

        flightPath.setMap(map);
        setCurrentLocation($("#rangeInputHistory").val());
    }

    // Получает координаты машины по филтьтру
    function getDataForDeviceWithFilter(filter) {
        $.ajax({
            url: "/Dashboard/GetDataForDeviceWithFilter",    
            data: filter,
            type: "GET",
            success: function (response) {
                
                responseData = response;
                
                flightPlanCoordinates = [];
                temp1 = [];
                temp2 = [];
                temp3 = [];
                speed = [];
                upsstatus = [];
                distance = [];

                //console.log(response)

                response.forEach(e => {
                    switch (e.valueid.slice(e.valueid.lastIndexOf(".") + 1)) {
                        case "Temp1": { temp1.push({ datetime: e.datetime, value: e.value[0] }); break; }
                        case "Temp2": { temp2.push({ datetime: e.datetime, value: e.value[0] }); break; }
                        case "Temp3": { temp3.push({ datetime: e.datetime, value: e.value[0] }); break; }
                        case "speed": { speed.push({ datetime: e.datetime, value: e.value[0] }); break; }
                        case "upsstatus": { upsstatus.push({ datetime: e.datetime, value: e.value[0] }); break; }
                        case "position": {
                            if (parseFloat(e.value[0].substring(0, e.value[0].indexOf(".") + 5)) > 1 && parseFloat(e.value[1].substring(0, e.value[1].indexOf(".") + 5)) > 1){
                                flightPlanCoordinates.push({ lat: parseFloat(e.value[0].substring(0, e.value[0].indexOf(".") + 5)), lng: parseFloat(e.value[1].substring(0, e.value[1].indexOf(".") + 5)) });
                            }
                            
                            break;
                        }
                        case "Distance": { distance.push({ datetime: e.datetime, value: e.value[0] }); break; }
                    }
                    //flightPlanCoordinates.push(e.Location);
                });

                $("#rangeInputHistory").attr("max", flightPlanCoordinates.length - 2);

                var tst = [];
                speed.forEach(e => { if (parseFloat(e.value) > 0) tst.push(e)});

                console.log(tst);
                drawPath(flightPlanCoordinates);
            },
            error: function (error) {
                console.log(`error ${error}`);
            }
        });
    }


