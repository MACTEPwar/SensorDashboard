{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "./",
    "module": "es2015"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "files": [
    "./app.ts"
  ],
  "compileOnSave": true
}